@model List<InsuranceProject.Context.InsuranceList>

@{
    ViewData["Title"] = "sonuclar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container">
    <div class="header">
        <h1>
            Arama Sonuçları
        </h1>
    </div>
    <div class="row d-flex justify-content-center" style="margin:0">
        <div class="col-md-8">
            <form action="/arama">
                <div class="form-group d-flex">
                    <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                    <button type="submit" class="btn btn-primary search-button">Ara</button>
                </div>
            </form>
        </div>
    </div>
</div>

<div class="row d-flex justify-content-center" style="margin:0">
    <div class="col-md-8">
        @foreach (var item in Model)
        {
        @if (item.Name.Count() == 0)
        {
        <span>aksdas</span>
        }
        else
        {
        @item.Name
        }
        }
    </div>
</div>